{
  "name": "toastdesign/toast-theme",
  "description": "A Craft CMS starter project by Sean Smith",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "type": "project",
  "license": "MIT",
  "minimum-stability": "beta",
  "require": {
    "clubstudioltd/craft-asset-rev": "^7.0",
    "craftcms/ckeditor": "^3.6",
    "craftcms/cms": "^4.4.0",
    "mmikkel/cp-field-inspect": "^1.4",
    "nystudio107/craft-emptycoalesce": "^4.0",
    "nystudio107/craft-templatecomments": "^4.0",
    "putyourlightson/craft-sprig": "^2.7",
    "sebastianlenz/linkfield": "^2.1",
    "studioespresso/craft-dumper": "^3.0",
    "topshelfcraft/environment-label": "^4.0",
    "vaersaagod/matrixmate": "^2.1",
    "verbb/knock-knock": "^2.0",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
